import com.mbenabda.techwatch.testES.facts.answers.Age
import com.mbenabda.techwatch.testES.facts.age.*

rule "person is young"
when
    YouthLimitAge( $limit : value)
    Age(value <= $limit)
then
    insert(new IsYoung());
end

rule "person is old"
when
    YouthLimitAge( $limit : value)
    Age(value > $limit)
then
    insert(new IsOld());
end
